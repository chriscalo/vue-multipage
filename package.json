{
  "name": "vue-multipage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:pages dev:server:watch",
    "dev:pages": "nodemon --watch src/pages --watch scripts/generate-entry-points.js scripts/generate-entry-points.js",
    "dev:server": "vue-cli-service serve",
    "dev:server:watch": "nodemon --watch vue.config.js --watch 'src/**/*.js' --watch package.json --exec 'yarn run dev:server'",
    "build": "vue-cli-service build",
    "build:watch": "vue-cli-service build --watch",
    "serve": "http-server dist/ -c-1",
    "start": "http-server dist/"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "body-parser": "^1.18.3",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.5",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "fs-extra": "^7.0.1",
    "http-server": "^0.11.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prefetch": "^0.2.1",
    "ramda": "^0.26.1",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "common-tags": "^1.8.0",
    "fast-glob": "^2.2.6",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  }
}
